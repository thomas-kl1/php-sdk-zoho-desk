{
    "name": "tklein/php-sdk-zoho-desk",
    "description": "PHP SDK for Zoho Desk API v1 integration.",
    "type": "library",
    "keywords": [
        "php",
        "sdk",
        "client",
        "oauth",
        "zoho",
        "desk",
        "zoho-desk",
        "support",
        "ticket",
        "service",
        "customer-service",
        "customer"
    ],
    "homepage": "https://github.com/thomas-kl1/php-sdk-zoho-desk",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Klein",
            "email": "thomasklein876@gmail.com",
            "homepage": "https://www.linkedin.com/in/thomas-klein/",
            "role": "lead"
        }
    ],
    "support": {
        "source": "https://github.com/thomas-kl1/php-sdk-zoho-desk",
        "issues": "https://github.com/thomas-kl1/php-sdk-zoho-desk/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.10",
        "pdepend/pdepend": "^2.5",
        "phpstan/phpstan": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Zoho\\Desk\\": "src/",
            "Zoho\\OAuth\\": "lib/zoho/oauth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zoho\\Desk\\": "tests/",
            "Zoho\\OAuth\\": "lib/zoho/oauth/"
        }
    }
}
